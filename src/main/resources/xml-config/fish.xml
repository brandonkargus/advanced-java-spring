<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="northernPike" class="platform.codingnomads.co.corespring.lab.Fish">
        <property name="species" value="Northern Pike"/>
        <property name="size" value="38 inch"/>
    </bean>

    <bean id="walleye" class="platform.codingnomads.co.corespring.lab.Fish">
        <property name="species" value= "Walleye"/>
        <property name="size" value= "29 inch"/>
    </bean>
</beans>
